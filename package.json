    "drizzle-kit": "^0.24.1",
    "eslint": "~9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
